{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "indent": "off",
        "import/extensions": "off",
        "quotes": "off",
        "no-console": 0,
        "no-param-reassign": "off"
    }
}
